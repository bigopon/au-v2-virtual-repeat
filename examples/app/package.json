{
  "name": "aurelia-v2-virtual-repeat-demo",
  "description": "An demo application for aurelia 2 virtual repeat.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "???"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@aurelia/webpack-loader": "dev",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^10.2.1",
    "browser-do": "^3.0.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "htmlhint": "^0.14.2",
    "jasmine-core": "^3.6.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-shell-plugin-next": "^2.1.1"
  },
  "scripts": {
    "lint:js": "eslint src test --ext .js,.ts",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html",
    "test": "npm run test:headless",
    "pretest": "npm run lint && npm run build:test",
    "start": "webpack serve",
    "build": "rimraf dist && rollup -c",
    "build:types": "tsc --outDir dist/types --declaration --emitDeclarationOnly",
    "postbuild": "npm run build:types",
    "analyze": "rimraf dist && webpack --env production --analyze",
    "build:test": "rimraf dist && webpack --env test",
    "test:watch": "rimraf dist && webpack --env test --watch --runTest",
    "test:headless": "browser-do --jasmine < dist/entry-bundle.js",
    "test:chrome": "browser-do --jasmine --browser chrome < dist/entry-bundle.js",
    "deploy": "npm run build && cd dist && vercel --prod --name au-v2-virtual-repeat-2 --public --confirm"
  }
}
