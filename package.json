{
  "name": "aurelia-v2-virtual-repeat",
  "description": "An Aurelia 2 virtual repeat implementation.",
  "version": "0.1.3",
  "keywords": [
    "aurelia",
    "virtual repeat",
    "virtualization",
    "infinite scrolling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bigopon/aurelia-v2-virtual-repeat"
  },
  "bugs": "https://github.com/bigopon/aurelia-v2-virtual-repeat/issues",
  "main": "dist/cjs/aurelia-v2-virtual-repeat.js",
  "module": "dist/es2017/aurelia-v2-virtual-repeat.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@aurelia/kernel": "^2.0.0-alpha.2",
    "@aurelia/runtime": "^2.0.0-alpha.2",
    "@aurelia/runtime-html": "^2.0.0-alpha.2"
  },
  "devDependencies": {
    "@aurelia/testing": "^2.0.0-alpha.2",
    "@aurelia/webpack-loader": "^2.0.0-alpha.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "aurelia": "^2.0.0-alpha.2",
    "autoprefixer": "^10.2.1",
    "browser-do": "^3.0.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "htmlhint": "^0.14.2",
    "jasmine-core": "^3.7.1",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.5.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "standard-version": "^9.2.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "vercel": "^21.3.3",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-shell-plugin-next": "^2.1.1"
  },
  "scripts": {
    "lint": "eslint src test --ext .js,.ts",
    "build": "rimraf dist && rollup -c",
    "build:types": "tsc --outDir dist/types --declaration --emitDeclarationOnly",
    "build:demo": "cd examples/app && npm run build",
    "postbuild": "npm run build:types",
    "test": "karma start --singleRun --browsers ChromeHeadless",
    "test:watch": "karma start --browsers ChromeHeadless",
    "test:debugger": "karma start",
    "precut-release": "npm run test && npm run build",
    "cut-release": "git add dist && standard-version -t \"\" -i doc/CHANGELOG.md --commit-all",
    "cut-release:test": "standard-version -t \"\" -i doc/CHANGELOG.md --dry-run"
  }
}
